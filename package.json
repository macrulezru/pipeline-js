{
  "name": "rest-pipeline-js",
  "version": "1.1.5",
  "description": "Pipeline Orchestration Utilities for JavaScript REST API Clients",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:all",
    "build:all": "tsc --build",
    "build:vue": "tsc --project src/vue/tsconfig.json --outDir dist/vue",
    "build:react": "tsc --project src/react/tsconfig.json --outDir dist/react",
    "test": "vitest run",
    "demo:vue": "npm run start-demo-vite",
    "start-demo-vite": "vite --config vite.config.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "rest",
    "api",
    "client",
    "pipeline"
  ],
  "author": "macrulez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/macrulezru/pipeline-js"
  },
  "homepage": "https://macrulez.ru/#/en",
  "devDependencies": {
    "@testing-library/react": "^16.3.1",
    "@types/react": "^19.2.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^27.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "scheduler": "^0.27.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "vue": "^3.5.26",
    "vue-eslint-parser": "^10.2.0"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}

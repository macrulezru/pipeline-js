{
  "name": "rest-pipeline-js",
  "version": "1.0.9",
  "description": "Pipeline Orchestration Utilities for JavaScript REST API Clients",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vue": {
      "import": "./vue/distr/vue/index.js",
      "require": "./vue/distr/vue/index.js",
      "types": "./vue/distr/vue/index.d.ts"
    },
    "./vue/*": {
      "import": "./vue/distr/vue/*.js",
      "require": "./vue/distr/vue/*.js",
      "types": "./vue/distr/vue/*.d.ts"
    },
    "./react": {
      "import": "./react/distr/react/index.js",
      "require": "./react/distr/react/index.js",
      "types": "./react/distr/react/index.d.ts"
    },
    "./react/*": {
      "import": "./react/distr/react/*.js",
      "require": "./react/distr/react/*.js",
      "types": "./react/distr/react/*.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:vue && npm run build:react && tsc",
    "build:vue": "tsc --project vue/tsconfig.json --outDir vue/distr",
    "build:react": "tsc --project react/tsconfig.json --outDir react/distr",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "rest",
    "api",
    "client",
    "pipeline"
  ],
  "author": "macrulez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/macrulezru/pipeline-js"
  },
  "homepage": "https://macrulez.ru/#/en",
  "devDependencies": {
    "@types/react": "^19.2.8",
    "react": "^19.2.3",
    "typescript": "^5.9.3",
    "vue": "^3.5.26"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
